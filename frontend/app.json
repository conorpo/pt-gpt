{
  "expo": {
    "name": "pt-gpt.io",
    "slug": "pt-gpt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/imgs/logos/favicon/favicon.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/imgs/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*.{png,jpg,jpeg,gif,svg,ttf,otf,woff,woff2}",
      "./assets/fonts/*.",
      "./assets/imgs/*.{png}",
      "./assets/imgs/logos/*.{png}",
      "./assets/imgs/logos/favicon/*.{png,ico}",
      "./assets/imgs/personalities/*.{png}"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.conorpo.ptgpt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.conorpo.ptgpt"
    },
    "web": {
      "favicon": "./assets/imgs/logos/favicon/favicon-32x32.png"
    },
    "extra": {
      "eas": {
        "projectId": "b8646f20-9f35-4d6b-9593-612d553554c1"
      }
    }
  }
}
